{
  "name": "mocha-mix",
  "version": "0.1.7",
  "description": "An easy way to test and mock your React Components",
  "main": "./index.js",
  "scripts": {
    "test": "istanbul cover _mocha --require ./mocha-processor.js test examples/tests",
    "testWatch": "mocha --watch --require ./mocha-processor.js test examples/tests",
    "mocha": "mocha --require ./mocha-processor.js test",
    "examples": "mocha --require ./mocha-processor.js examples/tests"
  },
  "author": {
    "name": "RexK",
    "email": "myungsunrex@gmail.com"
  },
  "keywords": [
    "react",
    "jest",
    "mocha",
    "spy",
    "mockery",
    "sinon",
    "fast",
    "unit testing"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rexk/mocha-mix"
  },
  "peerDependencies": {
    "mockery": "^1.4",
    "react": "^0.13.3",
    "react-tools": "^0.13.3",
    "sinon": "^1.15",
    "jsdom": ">=3.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "eslint": "^1.7.3",
    "eslint-plugin-react": "^3.6.3",
    "expect": "^1.12.0",
    "istanbul": "^0.3.22",
    "jsdom": "^3.1.2",
    "mocha": "^2.3.3",
    "mockery": "^1.4.0",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "sinon": "^1.17.1"
  }
