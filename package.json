    "fast",
    "unit testing"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rexk/mocha-mix"
  },
  "peerDependencies": {
    "babel-core": "^5.x",
    "jsdom": "^3.1.x",
    "mockery": "*",
    "react": "*",
    "react-tools": "*",
    "sinon": "*",
    "react-router": "*"
  },
  "devDependencies": {
    "babel-core": "5.6.x",
