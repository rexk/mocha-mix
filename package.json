{
  "name": "mocha-mix",
  "version": "0.1.7",
  "description": "An easy way to test and mock your React Components",
  "main": "./index.js",
  "scripts": {
    "test": "echo \"ok\"",
    "test:cover": "istanbul cover _mocha --require ./mocha-processor.js test examples/tests",
    "test:watch": "mocha --watch --require ./mocha-processor.js test examples/tests",
    "mocha": "mocha --require ./mocha-processor.js test",
    "examples": "mocha --require ./mocha-processor.js examples/tests"
  },
  "author": {
    "name": "RexK",
    "email": "myungsunrex@gmail.com"
  },
  "keywords": [
    "react",
    "jest",
    "mocha",
    "spy",
    "mockery",
    "sinon",
    "fast",
    "unit testing"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rexk/mocha-mix"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.15.0",
    "mocha": "^2.3.4",
    "mocha-mix-jsdom": "0.0.0-rc1",
    "mocha-mix-mockery": "0.0.0-rc",
    "mocha-mix-react": "0.0.0-rc"
  },
  "dependencies": {
    "lodash.isempty": "^4.0.0",
    "lodash.isobject": "^3.0.2"
  }
}
