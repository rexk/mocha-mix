{
  "name": "mocha-mix",
  "version": "1.0.0-alpha.0",
  "description": "An easy way to test and mock your React Components",
  "main": "./index.js",
  "scripts": {
    "test": "echo \"ok\"",
    "test:cover": "istanbul cover _mocha --require ./mocha-processor.js test examples/tests",
    "test:watch": "mocha --watch $(find ./src -name *.test.js)",
    "mocha": "mocha --require ./mocha-processor.js test",
    "examples": "mocha --require ./mocha-processor.js examples/tests"
  },
  "author": {
    "name": "RexK",
    "email": "myungsunrex@gmail.com"
  },
  "keywords": [
    "react",
    "jest",
    "mocha",
    "spy",
    "mockery",
    "sinon",
    "fast",
    "unit testing"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rexk/mocha-mix"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.15.0",
    "expect": "^1.13.4",
    "mocha": "^2.3.4",
    "mocha-mix-mockery": "0.0.1-alpha.2",
    "sinon": "^1.17.2"
  },
  "dependencies": {
    "lodash.assign": "^4.0.0",
    "lodash.isempty": "^4.0.0",
    "lodash.isfunction": "^3.0.7",
    "lodash.isnull": "^3.0.0",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.0",
    "lodash.isundefined": "^3.0.1"
  }
}
